@model LoginUser
<h2>Login Here</h2>
<form asp-action="Login" asp-controller="Home" method="post">
    <div class="form-group">
        <span asp-validation-for="log_email"></span>
        <label asp-for="log_email"></label>
        <input asp-for="log_email">
    </div>
    <div class="form-group">
        <span asp-validation-for="log_password"></span>
        <label asp-for="log_password"></label>
        <input asp-for="log_password">
    </div>
    <input type="submit" value="Login">
</form>
